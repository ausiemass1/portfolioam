<section class="container mt-4">
    <h1><%= title %></h1>
  
    <% if (users.length === 0) { %>
      <p>No users found.</p>
    <% } else { %>
      <table class="table table-striped table-bordered mt-3">
        <thead class="table-dark">
          <tr>
            <th>Name</th>
            <th>Age</th>
            <th>Email</th>
          </tr>
        </thead>
        <tbody>
          <% users.forEach(user => { %>
            <tr>
              <td><%= user.name %></td>
              <td><%= user.age %></td>
              <td><%= user.email %></td>
            </tr>
          <% }) %>
        </tbody>
      </table>
    <% } %>
  
    <hr>
  

  </section>
  